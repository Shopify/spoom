<div>
  <h3><%= @symbol.fully_qualified_name %></h3>
  <% if @symbol.comment_string %>
    <p><%= @symbol.comment_string %></p>
  <% end %>
  <% if @symbol.sig_string %>
    <pre><code><%= @symbol.sig_string %></code></pre>
  <% end %>
  <% if @symbol.is_a?(Spoom::Model::Scope) %>
    <% @symbol.symbols.each do |symbol| %>
      <%= symbol_card(symbol).html %>
    <% end %>
  <% end %>
</div>
